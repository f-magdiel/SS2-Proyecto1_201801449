      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{738656A9-82C7-4266-969E-C26C804A8498}"
          SQLTask:SqlStatementSource="USE seminario2_201801449;&#xA;INSERT INTO venta(id_sucursal, id_cliente, id_vendedor, id_producto, id_fecha, unidades, precio_unitario)&#xA;SELECT sucursal.id_sucursal, cliente.id_cliente, vendedor.id_vendedor, producto.id_producto, fecha.id_fecha, v.unidades, v.PrecioUnitario &#xA;FROM sucursal&#xA;INNER JOIN PIVOTE.dbo.venta v on v.CodSucursal= sucursal.codigo&#xA;INNER JOIN producto on v.CodProducto = producto.codigo&#xA;INNER JOIN cliente on v.CodigoCliente = cliente.codigo&#xA;INNER JOIN vendedor on v.CodVendedor = vendedor.codigo&#xA;INNER JOIN fecha on v.fecha = format(fecha.fecha, 'dd/MM/yyyy');&#xA;DELETE FROM venta WHERE unidades &lt; 0 OR precio_unitario &lt; 0.0;&#xA;&#xA;&#xA;INSERT INTO compra(id_sucursal, id_proveedor, id_producto, id_fecha, unidades, precio_unitario)&#xA;SELECT sucursal.id_sucursal, proveedor.id_proveedor, producto.id_producto, fecha.id_fecha, c.unidades, c.CostoU&#xA;FROM sucursal&#xA;INNER JOIN PIVOTE.dbo.compra c on c.CodSucursal= sucursal.codigo&#xA;INNER JOIN producto on c.CodProducto = producto.codigo&#xA;INNER JOIN proveedor on c.CodProveedor = proveedor.codigo&#xA;INNER JOIN fecha on c.fecha = format(fecha.fecha, 'dd/MM/yyyy');&#xA;DELETE FROM compra WHERE unidades &lt; 0 OR precio_unitario &lt; 0.0; "
          SQLTask:ServerVersion="9"
          SQLTask:ExtendedLogging="True"
          SQLTask:LocalConnectionForLogging=""
          SQLTask:TaskName=""
          SQLTask:IgnoreDatabasesInNotOnlineState="False" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />

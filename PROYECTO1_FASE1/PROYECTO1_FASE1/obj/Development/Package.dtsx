      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{738656A9-82C7-4266-969E-C26C804A8498}"
          SQLTask:SqlStatementSource="USE seminario2_201801449;&#xA;&#xA;DELETE  a&#xA;FROM  cliente a&#xA;        LEFT JOIN&#xA;        (&#xA;            SELECT MIN(id_cliente) id_cliente, codigo, nombre, direccion, numero, tipo&#xA;            FROM  cliente&#xA;            GROUP   BY codigo, nombre, direccion, numero, tipo&#xA;        ) b ON  a.id_cliente = b.id_cliente AND&#xA;                a.codigo = b.codigo AND&#xA;                a.nombre = b.nombre AND&#xA;                a.direccion = b.direccion AND&#xA;                a.numero = b.numero AND&#xA;                a.tipo = b.tipo&#xA;WHERE   b.id_cliente IS NULL;&#xA;&#xA;DELETE  a&#xA;FROM    vendedor a&#xA;        LEFT JOIN&#xA;        (&#xA;            SELECT MIN(id_vendedor) id_vendedor, codigo, nombre, vacacionista&#xA;            FROM    vendedor&#xA;            GROUP   BY codigo, nombre, vacacionista&#xA;        ) b ON  a.id_vendedor = b.id_vendedor AND&#xA;                a.codigo = b.codigo AND&#xA;                a.nombre = b.nombre AND&#xA;                a.vacacionista = b.vacacionista&#xA;WHERE   b.id_vendedor IS NULL;&#xA;&#xA;DELETE  a&#xA;FROM    producto a&#xA;        LEFT JOIN&#xA;        (&#xA;            SELECT MIN(id_producto) id_producto, codigo, nombre, categoria, marca&#xA;            FROM    producto&#xA;            GROUP   BY codigo, nombre, categoria, marca&#xA;        ) b ON  a.id_producto = b.id_producto AND&#xA;                a.codigo = b.codigo AND&#xA;                a.nombre = b.nombre AND&#xA;                a.categoria = b.categoria AND&#xA;                a.marca = b.marca&#xA;WHERE   b.id_producto IS NULL;&#xA;&#xA;&#xA;DELETE  a&#xA;FROM    sucursal a&#xA;        LEFT JOIN&#xA;        (&#xA;            SELECT MIN(id_sucursal) id_sucursal, codigo, nombre, direccion, departamento, region&#xA;            FROM    sucursal&#xA;            GROUP   BY codigo, nombre, direccion, departamento, region&#xA;        ) b ON  a.id_sucursal = b.id_sucursal AND&#xA;                a.codigo = b.codigo AND&#xA;                a.nombre = b.nombre AND&#xA;                a.direccion = b.direccion AND&#xA;                a.departamento = b.departamento AND&#xA;                a.region = b.region&#xA;WHERE   b.id_sucursal IS NULL;&#xA;&#xA;&#xA;DELETE  a&#xA;FROM    fecha a&#xA;        LEFT JOIN&#xA;        (&#xA;            SELECT MIN(id_fecha) id_fecha, fecha&#xA;            FROM    fecha&#xA;            GROUP   BY fecha&#xA;        ) b ON  a.id_fecha = b.id_fecha AND&#xA;                a.fecha = b.fecha&#xA;WHERE   b.id_fecha IS NULL;&#xA;&#xA;&#xA;DELETE  a&#xA;FROM    proveedor a&#xA;        LEFT JOIN&#xA;        (&#xA;            SELECT MIN(id_proveedor) id_proveedor, codigo, nombre, direccion, numero, web&#xA;            FROM    proveedor&#xA;            GROUP   BY codigo, nombre, direccion, numero, web&#xA;        ) b ON  a.id_proveedor = b.id_proveedor AND&#xA;                a.codigo = b.codigo AND&#xA;                a.nombre = b.nombre AND&#xA;                a.direccion = b.direccion AND&#xA;                a.numero = b.numero AND&#xA;                a.web = b.web&#xA;WHERE   b.id_proveedor IS NULL;&#xA;"
          SQLTask:ServerVersion="9"
          SQLTask:ExtendedLogging="True"
          SQLTask:LocalConnectionForLogging=""
          SQLTask:TaskName=""
          SQLTask:IgnoreDatabasesInNotOnlineState="False" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
